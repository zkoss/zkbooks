<?xml version="1.0" encoding="UTF-8"?>
<zk xmlns="http://www.zkoss.org/2005/zk/config"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.zkoss.org/2005/zk/config http://www.zkoss.org/2005/zk/config/zk.xsd">

    <preference>
        <name>org.zkoss.zk.ui.WebApp.name</name>
        <value>reference.component</value>
    </preference>
    <client-config>
        <debug-js>true</debug-js>
        <!--<enable-source-map>true</enable-source-map>-->
    </client-config>
    <!-- for embedded demo in Component Reference -->
    <library-property>
        <name>org.zkoss.web.servlet.http.embedded.enabled</name>
        <value>true</value>
    </library-property>
    <library-property>
        <name>org.zkoss.gmaps.apiKey</name>
        <value>your_key</value>
    </library-property>
</zk>
